    org.apache.tika/tika-core {:mvn/version "1.24"} ; for contrib.datafy-fs file type detection
    metosin/malli {:mvn/version "0.14.0"}
    com.datomic/peer {:mvn/version "1.0.7075"}
    clj-jgit/clj-jgit {:mvn/version "1.0.2"} ; git browser
    }}

  :heroicons
  {:extra-deps
   {com.hyperfiddle/heroicons-electric {:mvn/version "0327158"}}}
  :heroicons-local-deps ; experts only - requires `git submodule update --init --recursive`
  {:extra-paths ["vendor/heroicons-electric/target/gen"]} ; make sure you run the build script inside the submodule

  }}
