    org.clojure/core.match {:mvn/version "1.1.0"} ; staffly, todos
    org.apache.tika/tika-core {:mvn/version "1.24"} ; explorer: datafy-fs
    }
   :override-deps ; uberjar sees only this one alias
   {com.hyperfiddle/electric {:local/root "vendor/electric"}
    com.hyperfiddle/hyperfiddle {:local/root "vendor/hyperfiddle"}}}

  :optimistic-todomvc
  {:extra-deps
   {datascript/datascript {:mvn/version "1.6.1"}}}

  :datomic-browser
